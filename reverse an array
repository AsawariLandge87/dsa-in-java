

public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		int[] arr={4, 1, 7, 7, 5};
		int left=0;
		int right= arr.length-1;
		//reverse array
		while(left<right){
		    int temp=arr[left];
		    arr[left]=arr[right];
		    arr[right]=temp;
		    
		    left++;
		    right--;
		    
		}
		
		System.out.println("Reversed array: ");
		for(int i=0; i<arr.length;i++){
		    System.out.print(arr[i]+" ,");
		}
		
	}
}
